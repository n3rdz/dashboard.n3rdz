<script type='text/javascript'>
$(function() {
  // These settings override the defaults set in application.coffee. You can do this on a per dashboard basis.
  Dashing.gridsterLayout('[{"col":1,"row":1,"size_x":1,"size_y":1},{"col":1,"row":2,"size_x":2,"size_y":1},{"col":2,"row":1,"size_x":1,"size_y":1},{"col":3,"row":1,"size_x":3,"size_y":2},{"col":1,"row":3,"size_x":5,"size_y":1}]')
  Dashing.widget_base_dimensions = [370, 340]
  Dashing.numColumns = 5
});
</script>


<% content_for :title do %>theo 1080p dashboard<% end %>

<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="SAD_Clock" style="background-color:#666666;"></div>
      <i class="fa fa-clock-o icon-background"></i>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="twitter_mentions" data-view="Tweets" style="background-color:#666666;" data-moreinfo=""></div>
      <i class="fa fa-twitter icon-background"></i>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1" ondblclick="location.href='https://twitter.com/DDgamechecker';">
      <div data-id="twitter_user_followers" data-view="Number" data-title="Twitter Followers" style="background-color: #00aced"></div>
      <i class="fa fa-twitter icon-background"></i>
    </li>

      <li data-row="1" data-col="1" data-sizex="3" data-sizey="2">
       <div data-id="google_calendar" data-view="GoogleCalendar" style=" background-color:#666666;"></div>
     <i class="fa fa-calendar icon-background"></i>
    </li>

    <li data-row="1" data-col="2" data-sizex="5" data-sizey="1" ondblclick="location.href='https://www.youtube.com/playlist?list=PLcqQhtn0kD2luzvzwePIs9I40I911putW';">
      <div data-id="youtube_video_views" data-view="List" data-unordered="true" data-title="Popular YouTube videos" style="background-color: #c0392b"></div>
      <i class="fa fa-youtube-play icon-background"></i>
    </li>

  </ul>
</div>